@article{Bildsten:1998,
	Abstract = {Recent discoveries by the Rossi X-Ray Timing Explorer indicate that most of the rapidly accreting ( ##IMG## [http://ej.iop.org/icons/Entities/dotM.gif] {dot M} ##IMG## [http://ej.iop.org/icons/Entities/gtrsim.gif] {gtrsim} 10 −11 M ##IMG## [http://ej.iop.org/images/1538-4357/501/1/L89/img3.gif] {_{{\odot}}} yr −1 ) weakly magnetic ( B ##IMG## [http://ej.iop.org/icons/Entities/ll.gif] {ll} 10 11 G) neutron stars in the Galaxy are rotating at spin frequencies ν s ##IMG## [http://ej.iop.org/icons/Entities/gtrsim.gif] {gtrsim} 250 Hz. Remarkably, they all rotate in a narrow range of frequencies (no more than a factor of 2, with many within 20% of 300 Hz). I suggest that these stars rotate fast enough so that, on average, the angular momentum added by accretion is lost to gravitational radiation. The strong ν s -dependence of the angular momentum loss rate from gravitational radiation then provides a natural reason for similar spin frequencies. Provided that the interior temperature has a large-scale asymmetry misaligned from the spin axis, then the temperature-sensitive electron captures in the deep crust can provide the quadrupole needed ( ~10 −7 MR 2 ) to reach this limiting situation at ν s ≈ 300 Hz. This quadrupole is only present during accretion and makes it difficult to form radio pulsars with ν s > (600-800) Hz by accreting at ##IMG## [http://ej.iop.org/icons/Entities/dotM.gif] {dot M} ##IMG## [http://ej.iop.org/icons/Entities/gtrsim.gif] {gtrsim} 10 −10 M ##IMG## [http://ej.iop.org/images/1538-4357/501/1/L89/img3.gif] {_{{\odot}}} yr −1 . The gravity wave strength is h c ~(0.5-1) × 10 −26 from many of these neutron stars and greater than 2 × 10 −26 for Sco X-1. Prior knowledge of the position, spin frequency, and orbital periods will allow for deep searches for these periodic signals with gravitational wave interferometers (LIGO, VIRGO, and the "dual-recycled" GEO 600 detector), and experimenters need to take such sources into account. Sco X-1 will most likely be detected first.},
	Author = {Lars Bildsten},
	Date-Added = {2012-01-15 01:09:26 -0800},
	Date-Modified = {2012-01-22 20:42:00 -0800},
	Journal = {The Astrophysical Journal Letters},
	Number = {1},
	Pages = {L89},
	Title = {Gravitational Radiation and Rotation of Accreting Neutron Stars},
	Volume = {501},
	Year = {1998},
	Bdsk-Url-1 = {http://stacks.iop.org/1538-4357/501/i=1/a=L89}}

