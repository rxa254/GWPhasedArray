@article{Schnupp:1985,
	Abstract = {In long-light-path laser interferometry, as used for gravitational wave detection, scattered light is a major noise source. Interferometers that employ optical delay lines are particularly susceptible to noise from scattered light. The authors present a method of suppressing such noise signals by using fast phase modulation of the laser light. It is shown that all scattered all contributions from the delay line may be cancelled, up to arbitrarily long path differences. This can be achieved with an appropriately structured square-wave modulation function which switches the phase of the light by pi .},
	Author = {L Schnupp and W Winkler and K Maischberger and A Rudiger and R Schilling},
